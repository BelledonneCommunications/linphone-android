<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/call_nav_graph"
    app:startDestination="@id/currentFragment">

    <fragment
        android:id="@+id/currentFragment"
        android:name="org.linphone.activities.voip.fragments.CurrentFragment"
        tools:layout="@layout/voip_current_fragment"
        android:label="CurrentFragment" >
        <action
            android:id="@+id/action_currentFragment_to_callsListFragment"
            app:destination="@id/callsListFragment" />
        <action
            android:id="@+id/action_currentFragment_to_callParamsFragment"
            app:destination="@id/callParamsFragment" />
    </fragment>

    <fragment
        android:id="@+id/callsListFragment"
        android:name="org.linphone.activities.voip.fragments.CallsListFragment"
        tools:layout="@layout/voip_calls_list_fragment"
        android:label="CallsListFragment" />

    <fragment
        android:id="@+id/callParamsFragment"
        android:name="org.linphone.activities.voip.fragments.CallParamsFragment"
        tools:layout="@layout/voip_call_params_fragment"
        android:label="CallParamsFragment" />

</navigation>